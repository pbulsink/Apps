goalie <- filter(goalie.bygame, Newcode %in% db.games == FALSE)
player <- filter(player.bygame, Newcode %in% db.games == FALSE)
combo <- filter(combos.bygame, Newcode %in% db.games == FALSE)
# Write tables / Ajouter les tables
